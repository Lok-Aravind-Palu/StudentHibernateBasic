<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>

	<class name="com.practice.hibernate.HibetnateExample.StudentDetails" table="StudentDetailTB">

		<id name="studentId" column="studentId" type="int">
			<generator class="identity"></generator>
		</id>


		<property name="studentName" column="studentName" type="string"></property>

		<property name="studentAge" column="studentAge" type="int"></property>


	</class>

</hibernate-mapping>

